    //gerak pada jarak tertentu
    if(distance_to_object(obHero) &lt;= 30)
    {
        sprite_index = spMusuh1a;
        //gerak ke hero, dengan kecepatan 3, dan sampai ke titik hero / melahap
        
        if(x mod 32 &lt; 3){
            x = (x div 32) * 32;
        }else if(x mod 32 &gt; 30){
            x = ((x + 3) div 32) * 32;
        }
        
        if(x &gt; obHero.x || x &lt; obHero.x){
            if(x &lt; obHero.x &amp;&amp; place_free(x + 3, y)){
                x += 3;
            }else if(x &gt; obHero.x &amp;&amp; place_free(x - 3, y)){
                x -= 3;
            }
        }
        
        if(y mod 32 &lt; 3){
            y = (y div 32) * 32;
        }else if(y mod 32 &gt; 30){
            y = ((y + 3) div 32) * 32;
        }
        
        if(y &gt; obHero.y || y &lt; obHero.y){
            if(y &lt; obHero.y &amp;&amp; place_free(x,y+3)){
                y += 3;
            }else if(y &gt; obHero.y &amp;&amp; place_free(x,y-3)){
                y -= 3;
            }
        }
    }
    else
    {
        sprite_index = spMusuh1;
        if(move &lt;= 1) //jika status ke bawah
        {
            y += 2;
        }else if(move &lt;= 2 &amp;&amp; move &gt; 1) //jika status ke atas
        {
            y -= 2;
        }else if(move &lt;= 3 &amp;&amp; move &gt;2) //jika status ke kiri
        {
            x -= 2;
            image_xscale = -1;
        }else //jika ke kanan
        {
            image_xscale = 1;
            x += 2;
        }
    }
}
