            <kind>1</kind>
            <string>var i = 0;
var statM = 0;
var atas = 0;
var bawah = 0;
var kiri = 0;
var kanan = 0;

if (!place_meeting(x, y - sprite_height, obDinding) &amp;&amp; (place_meeting(x, y - (2*sprite_height), obDinding) || place_meeting(x, y - (3*sprite_height), obDinding))) {
   //atas
   atas = 1;
   i++;
}

if (!place_meeting(x + sprite_width, y, obDinding) &amp;&amp; (place_meeting(x + (2*sprite_width), y, obDinding) || place_meeting(x + (3*sprite_width), y, obDinding))) {
   //kanan
   kanan = 1;
   i++;
}
    //kiri
if (!place_meeting(x - sprite_width, y, obDinding) &amp;&amp; (place_meeting(x - (2*sprite_width), y, obDinding) || place_meeting(x - (3*sprite_width), y, obDinding))) {
    kiri = 1;
    i++;
}
    //bawah
if (!place_meeting(x, y + sprite_height, obDinding) &amp;&amp; (place_meeting(x, y + (2*sprite_height), obDinding) || place_meeting(x, y + (3*sprite_height), obDinding))) {
    bawah = 1;
    i++;
}

if(i &gt;= 3){
    while(statM == 0){
        pindah = random_range(0,1);
        if(pindah &gt;= 0 &amp;&amp; pindah &lt; 0.25){
            if(atas == 1){
                move = 2;
                statM = 1;
            }
        }
        if(pindah &gt; 0.25 &amp;&amp; pindah &lt;= 0.5){
            if(bawah == 1){
                move = 1;
                statM = 1;
            }
        }
        if(pindah &gt; 0.5 &amp;&amp; pindah &lt;= 0.75){
            if(kiri == 1){
                move = 3;
                statM = 1;
            }
        }
        if(pindah &gt; 0.75 &amp;&amp; pindah &lt;= 1){
            if(kanan == 1){
                move = 4;
                statM = 1;
            }
        }
    }        
}
</string>
