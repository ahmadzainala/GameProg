        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>var tempX = x - obHero.x;
var tempY = y - obHero.y;
if(tempX &lt; 0)
{
    tempX = tempX * (-1);
}
if(tempY &lt; 0)
{
    tempY = tempY * (-1);
}

if(tempX &lt; tempY &amp;&amp; statVertikal == 1) //jika jangkauan vertikal lebih jauh dari horizontal
{
    if (y &lt; obHero.y &amp;&amp; statBawah == 1) //hero ada dibawah
    {
        statBawah = 0;
        move = 1; //pilih rute bawah
    }
    else if(y &gt; obHero.y &amp;&amp; statAtas == 1) //hero diatas
    {
        statAtas = 0;
        move = 2; //rute atas
    }
    statVertikal = 0;
    statHorizontal = 1;
    statKanan = 1;
    statKiri = 1;
}
else if(tempX &gt;= tempY &amp;&amp; statHorizontal == 1) //jika jangkauan vertikal lebih jauh dari horizontal
{
    if (x &gt; obHero.x &amp;&amp; statKiri == 1) //hero ada dibawah
    {
        statKiri = 0;
        move = 3; //pilih rute kiri
    }
    else if(x &lt; obHero.y &amp;&amp; statKanan == 1) //hero diatas
    {
        statKanan = 0;
        move = 4; //rute atas
    }
    statVertikal = 1;
    statHorizontal = 0;
    statAtas = 1;
    statBawah = 1;
}
else if (statVertikal == 1 &amp;&amp; statHorizontal == 0) //mentok ga ada jalur terdekat ke kanan/kiri
{
    
    if (statBawah == 1) //hero ada dibawah
    {
        statBawah = 0;
        move = 1; //pilih rute bawah
    }else //hero diatas
    {
        statAtas = 0;
        move = 2; //rute atas
    }
    if(statAtas == 0 &amp;&amp; statBawah == 0){
        statVertikal = 0;
        statHorizontal = 1;
    }
}
else if (statHorizontal == 1 &amp;&amp; statVertikal == 0) //mentok ga ada jalur terdekat keatas/kebawah
{
    
    if (statKiri == 1) //hero dikiri
    {
        statKiri = 0;
        move = 3;
    }
    else //hero dikanan
    {
        statKanan = 0;
        move = 4;
    }
    
    if(statKiri == 0 &amp;&amp; statKanan ==0)
    {
        statHorizontal = 0;
        statVertikal = 1;        
    }
}
else //awal coba jalur kiri atau kanan
{
    show_debug_message("ssssssss");
    if(statVertikal == 0 &amp;&amp; statHorizontal ==0)
    {
        statVertikal = 1;
        statAtas = 1;
        statBawah = 1;
        statHorizontal = 1;
        statKiri = 1;
        statKanan = 1;
    }
    
    if (x &gt; obHero.x) //hero dikiri
    {
        statKiri = 0;
        move = 3;
    }
    else //hero dikanan
    {
        statKanan = 0;
        move = 4;
    }
}
</string>
